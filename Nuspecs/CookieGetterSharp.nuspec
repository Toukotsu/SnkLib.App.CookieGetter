<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>SnkLib.App.CookieGetter.Sharp</id>
        <version>1.3.0.0</version>
        <title>SnkLib.App.CookieGetter.Sharp</title>
        <authors>namoshika</authors>
        <owners>namoshika</owners>
        <summary>This library provides compatibility with CookieGetterSharp to "SnkLib.App.CookieGetter".</summary>
        <projectUrl>https://github.com/namoshika/SnkLib.App.CookieGetter</projectUrl>
        <licenseUrl>https://www.gnu.org/licenses/lgpl-2.1.txt</licenseUrl>
        <description>SnkLib.App.CookieGetterに対してCookieGetterSharpとの互換性を持たせます。</description>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <copyright>Copyright 2014 namoshika</copyright>
        <tags>Browser Cookie Getter</tags>
        <references>
            <reference file="CookieGetterSharp.dll" />
        </references>
        <dependencies>
            <group targetFramework="net45">
                <dependency id="SnkLib.App.CookieGetter" version="1.3.0.0" />
            </group>
            <group targetFramework="net40">
                <dependency id="SnkLib.App.CookieGetter" version="1.3.0.0" />
                <dependency id="Microsoft.Net.Http" version="2.2.28" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <!--.net4.5-->
        <file src="..\Net4.5\CookieGetterSharp\bin\Release\CookieGetterSharp.dll" target="lib\net45" />
        <!--.net4.0-->
        <file src="..\Net4.0\CookieGetterSharp\bin\Release\CookieGetterSharp.dll" target="lib\net40" />
    </files>
</package>